<?xml version="1.0" encoding="utf-8" standalone="no"?><web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:web="http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" version="2.5" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/web-app_2_5.xsd">
	
	<servlet>
		<servlet-name>NostradamusWrite</servlet-name>
		<servlet-class>com.megadevs.nostradamus.backend.NostradamusWriteServlet</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>NostradamusWrite</servlet-name>
		<url-pattern>/nostradamus_write</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>NostradamusRead</servlet-name>
		<servlet-class>com.megadevs.nostradamus.backend.NostradamusReadServlet</servlet-class>
	</servlet>
	<servlet-mapping>
	<servlet-name>NostradamusRead</servlet-name>
		<url-pattern>/nostradamus_read</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>AddCameraEntry</servlet-name>
		<servlet-class>com.megadevs.nostradamus.backend.AddCameraEntry</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>AddCameraEntry</servlet-name>
		<url-pattern>/add_camera_entry</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetCameraSnapshot</servlet-name>
		<servlet-class>com.megadevs.nostradamus.backend.GetCameraSnapshot</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCameraSnapshot</servlet-name>
		<url-pattern>/get_camera_snapshot</url-pattern>
	</servlet-mapping>
	
	<servlet>
		<servlet-name>GetCameraInfo</servlet-name>
		<servlet-class>com.megadevs.nostradamus.backend.GetCameraInfo</servlet-class>
	</servlet>
	<servlet-mapping>
		<servlet-name>GetCameraInfo</servlet-name>
		<url-pattern>/get_camera_info</url-pattern>
	</servlet-mapping>
	
	<welcome-file-list>
		<welcome-file>index.html</welcome-file>
	</welcome-file-list>
 <servlet>
  <servlet-name>SystemServiceServlet</servlet-name>
  <servlet-class>com.google.api.server.spi.SystemServiceServlet</servlet-class>
  <init-param>
   <param-name>services</param-name>
   <param-value/>
  </init-param>
 </servlet>
 <servlet-mapping>
  <servlet-name>SystemServiceServlet</servlet-name>
  <url-pattern>/_ah/spi/*</url-pattern>
 </servlet-mapping>
</web-app>